{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Mirosoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": {
      "Name": "File",
      "Args": {
        "path": "./logs/log-.txt",
         "rollingInterval":   "Day"
      }
    },
  "AllowedHosts": "*"
}
